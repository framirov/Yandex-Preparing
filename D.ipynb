    "def brackets(s, a, b):\n",
    "    if (a + b == 2 * n):\n",
    "        print(s)\n",
    "    if a < n:\n",
    "        brackets(s + '(', a + 1, b)\n",
    "    if a > b:\n",
    "        brackets(s + ')', a, b + 1)\n",
    "\n",
    "n = int(input())\n",
    "brackets('',0,0)"
   ]
